{
  "name": "Objetivo",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "YearInicio": {
      "type": "date",
      "required": true
    },
    "YearFin": {
      "type": "date",
      "required": true
    },
    "Nombre": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "usuarios": {
      "type": "hasMany",
      "model": "Usuario",
      "foreignKey": "objetivo"
    },
    "ingresos": {
      "type": "hasMany",
      "model": "Ingreso",
      "foreignKey": "objetivo"
    },
    "pedidos": {
      "type": "hasMany",
      "model": "Pedido",
      "foreignKey": "objetivo"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "coordinador",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
